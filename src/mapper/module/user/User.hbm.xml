<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bktravel.modules.user.entity.BkUser" table="bk_userinfo" dynamic-insert="true" lazy="true">
		<cache usage="read-only" />
		<id name="id" type="java.lang.String">
			<column name="id" length="32" />
			<generator class="uuid.hex"></generator>
		</id>
		<many-to-one name="account" class="com.bktravel.sys.account.entity.BkAccount" lazy="proxy">
			<column name="accountId" length="32" />
		</many-to-one>
		<property name="name" type="java.lang.String">
			<column name="name" length="20" not-null="true" />
		</property>
		<property name="sex" type="java.lang.String">
			<column name="sex" length="2" not-null="true" />
		</property>
		<property name="age" type="java.lang.Integer">
			<column name="age" not-null="true" />
		</property>
		<property name="birthday" type="java.util.Date">
			<column name="birthday" length="19" not-null="true" />
		</property>
		<property name="card" type="java.lang.String">
			<column name="card" length="20" not-null="true" />
		</property>
	</class>
</hibernate-mapping>